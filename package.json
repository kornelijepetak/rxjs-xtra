{
    "name": "@fritech/rxjs-xtra",
    "author": "Kornelije Petak - Friday Technologies",
    "description": "An assorted set of goodies for use with rxjs.",
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kornelijepetak/rxjs-xtra.git"
    },
    "homepage": "https://github.com/kornelijepetak/rxjs-xtra#readme",
    "bugs": {
        "url": "https://github.com/kornelijepetak/rxjs-xtra/issues"
    },
    "keywords": [
